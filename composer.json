{
    "name": "kefisu/ziggy-bundle",
    "description": "Ziggy routing adapter made for Symfony",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": ">=6",
        "symfony/twig-bundle": ">=6"
    },
    "require-dev": {
        "pestphp/pest": "^2.8",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.10"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kefisu\\Bundle\\ZiggyBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Kevin Jansen",
            "email": "kevitius99@gmail.com",
            "role": "Maintainer"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
